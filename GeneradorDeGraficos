import pandas as pd
import numpy as np
import matplotlib.pyplot as plt

# Leer el archivo CSV
df = pd.read_csv('') #Archivo

# Reemplazar el valor 1 por NaN y eliminar filas con NaN

# Mostrar las primeras filas del DataFrame
print(df.head())

# Crear el gráfico de dispersión (solo puntos)
plt.figure(figsize=(10, 6))
plt.scatter(df['TamañoArreglo'], df['Tiempo'], color='b', marker='o')
plt.title('Gráfico de Datos')
plt.xlabel('Tamaño del Arreglo')
plt.ylabel('Tiempo de Ejecución (ms)')
plt.grid(True)
plt.show()
